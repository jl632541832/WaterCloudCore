        function gridList() {
            commonTable.rendertable({
                elem: '#currentTableId2',
                id: 'currentTableId2',
                url: '/OperationMonitoring/OrderDetails/GetGridJson',
                height: 200,
                toolbar: false,/*隐藏打印*/
