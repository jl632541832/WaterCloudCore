        function loadOne() {
            commonTable.rendertable({
                elem: '#currentTableId',
                id: 'currentTableId',
                url: "/OperationMonitoring/OrderDetails/GetGridJsonOne?keyvalue=" + $('#test1').val(),
                height: 880,
                toolbar: false,/*隐藏打印*/
                page: true,/*隐藏分页*/
