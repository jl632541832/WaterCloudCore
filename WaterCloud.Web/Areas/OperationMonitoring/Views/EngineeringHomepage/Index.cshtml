                series: [
                    {
                        name: '访问来源',
                        type: 'pie',
                        radius: '65%',
                        center: ['50%', '40%'],
                        data: str
                        ,
                        itemStyle: {  //图形样式
                            normal: { //normal 是图形在默认状态下的样式；emphasis 是图形在高亮状态下的样式，比如在鼠标悬浮或者图例联动高亮时。
                                label: {  //饼图图形上的文本标签
                                    show: true,  //平常不显示
                                    formatter: '{c}%',
                                    position: 'inner'
                                },
                                labelLine: {     //标签的视觉引导线样式
                                    show: false  //平常不显示
                                }
                            },
                            emphasis: {   //normal 是图形在默认状态下的样式；emphasis 是图形在高亮状态下的样式，比如在鼠标悬浮或者图例联动高亮时。
                                label: {  //饼图图形上的文本标签
                                    show: true,
                                    position: 'center',
                                    textStyle: {
                                        fontSize: '20',
                                        fontWeight: 'bold'
                                    }
                                },
                                itemStyle: {
                                    shadowBlur: 10,
                                    shadowOffsetX: 0,
                                    shadowColor: 'rgba(0, 0, 0, 0.5)'
                                }
                            }
                        }
                    }
                ]
            };
            myChart.setOption(option);
            // echarts 窗口缩放自适应
            window.onresize = function () {
                myChart.resize();
            }
        }
        function gridList() {
            common.rendertable({
                elem: '#currentTableId',
                id: 'currentTableId',
                url: '/OperationMonitoring/EngineeringHomepage/GetGridJson',
                height: 180,
                toolbar: false,/*隐藏打印*/
                page: false,/*隐藏分页*/
                sqlkey: 'id',//数据库主键
                cols: [[
                    { field: 'id', title: 'ID', sort: true, hide: true, hideAlways: true },
                    { field: 'MoldNo', title: '模具编号', width: 80, sort: true },
                    { field: 'Customers', title: '客户', width: 60, sort: true, filter: true },
                    { field: 'Type', title: '类型', width: 60, sort: true },
                    {
                        field: 'PlannedDeliveryDate', title: '计划交期', width: 80,
                        templet: function (d) {
                            if (d.PlannedDeliveryDate) {
                                return new Date(d.PlannedDeliveryDate).Format("yyyy-MM-dd");

                            }
                        }
                    },
                    {
                        field: 'EarlyWarning', title: '预警', align: 'center', width: 80,
                        templet: function (d) {
                            if (d.EarlyWarning) {
                                var temp = ''
                                var tip = d.EarlyWarning.split(';');
                                if (tip == 0) {
                                    return '<div class="layui-progress" style="margin-top: 10px;"><div class="layui-progress-bar layui-bg-red" lay-percent="100%"></div></div>';
                                }
                                else if (tip > 0) {
                                    var attnum = 1 - tip;
                                    return '<div class="layui-progress layui-bg-red" style="margin-top: 10px;"><div class="layui-progress-bar layui-bg-green" lay-percent=' + attnum * 100 + '%></div></div>';
                                }
                                else if (tip.length == 2) {
                                    return '<div class="layui-progress layui-bg-red" lay-percent=' + tip[0] * 100 + '% style="margin-top: 10px;"><div class="layui-progress-bar layui-bg-green" lay-percent=' + tip[1] * 100 + '%><div class="layui-progress-bar layui-bg-orange" lay-percent=' + tip[2] * 100 + '%></div></div></div>';
                                }
                            }
                        }
                    },
                ]]
                , done: function (res, currentCount) {
                    //***重点***：table渲染完成后渲染element进度条
                    element.render()
                }
            });
        }
        function gridListOn() {
            common.rendertable({
                elem: '#currentTableId2',
                id: 'currentTableId2',
                url: '/OperationMonitoring/EngineeringHomepage/GetGridJsonOn',
                height: 190,
                toolbar: false,/*隐藏打印*/
                page: false,/*隐藏分页*/
                sqlkey: 'id',//数据库主键
                cols: [[
                    {
                        field: 'Colour', title: '', width: 50, sort: false, filter: false,
                        templet: function (d) {
                            if (d.Colour) {
                                return '<span style="padding:1px 9px;border-radius:50%;background:' + d.Colour + ';"></span>'
                            }
                        }
                    },
                    //{ field: 'id', title: 'ID', sort: true, hide: true, hideAlways: true },
                    { field: 'MoldName', title: '模具名称', width: 120, sort: true },
                    { field: 'MoldNo', title: '模具编号', width: 120, sort: true },
                    { field: 'TN', title: 'TN', width: 120, sort: true },
                    { field: 'CustomerName', title: '客户', width: 120, sort: true },
                    { field: 'ContactPerson', title: '内部负责人', width: 120, sort: true },
                    { field: 'MoldType', title: '类型', width: 120, sort: true, filter: true },
                    { field: 'MoldState', title: '状态', width: 120, sort: true, filter: true },
                    {
                        field: 'MoldDate', title: '试模日期', width: 150,
                        templet: function (d) {
                            if (d.MoldDate) {
                                return new Date(d.MoldDate).Format("yyyy-MM-dd");

                            }
                        }
                    },
                    { field: 'MoldMaterial', title: '模仁材质', width: 120, sort: true },
                    { field: 'Category', title: '类别', width: 120, sort: true },
                ]]
            });
        }
        $(document).on('click', "#funOne", function () {
            //layer.msg("按钮点击");
            window.open("/Home/Index#//OperationMonitoring/EngineeringHomepage/Form");
        });
        $(document).on('click', "#funTwo", function () {
            window.open("/Home/Index#//OperationMonitoring/EngineeringHomepage/Form2");
        });
        $(document).on('click', "#funThree", function () {
            window.open("/Home/Index#//OperationMonitoring/EngineeringHomepage/Form3");
        });
    })
</script>
